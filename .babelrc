{
	"presets": [
    ["env", { "targets": { "node": "current", "useBuiltIns": true } }],
		["minify", { "mangle": false }]
  ],
	"plugins": [
    ["module-resolver", {
			"alias": {
				"APP": "./lib",
				"MAIN": "./lib/app",
				"SERVER": "./lib/server",
				"COMP": "./lib/controllers",
				"TEST": "./test"
			}
    }]
  ]
}